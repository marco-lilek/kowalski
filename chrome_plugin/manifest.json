{
	"manifest_version": 2,
	"name": "Kowalski",
	"description": "analysis",
	"version": "1.0",
	"author": "Marco Lilek",
	"icons": {"32": "icon.png"},
	"permissions": [
		"http://localhost/",
		"activeTab",
		"tabs",
		"declarativeContent"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_title": "Kowalski, analysis",
		"default_popup": "popup.html"
	}
}
